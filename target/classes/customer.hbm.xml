<?xml version="1.0"?>    
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ashokit.entities.CustomerEntity" table="CUSTOMERENTITIES">
		<id name="customerId" column="CUSTID" />
		<property name="customerName" column="CUSTNAME" length="20" />
		<property name="email" column="EMAIL" length="20" />
		<property name="contact" column="CONTACT" />
	</class>
</hibernate-mapping>